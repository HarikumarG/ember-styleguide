{{#if hasBlock}}
  {{yield}}
{{else}}
  <form class="navbar-form navbar-right searchbox" role="search">
    <div class="input-group">
      <label for="search-input" class="control-label sr-only">Search</label>
      <input type="text" class="form-control search input ds-input" placeholder="Search..." id="search-input"
        role="combobox" aria-expanded="false" aria-owns="algolia-autocomplete-listbox-0" autocorrect="off"
